INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (32aaa410-51d4-11e7-8dc1-3b9c08d048b3, f5a5e341-0700-4ed1-b591-76807ae1e4d1, 28116ffb-42bc-471b-9345-bac4adc2ddf7, 1493828630000, '330d83af-dede-4d21-989f-500fe360b853/28116ffb-42bc-471b-9345-bac4adc2ddf7/', False, 1525364630000, 91d07260-6c74-42d2-a9b4-1ec253f55ced, 'Progress Card Generation', 'Examination marks can be entered subject wise for each student and the total is calculated. Choose class, section and exam name, a mark list with student names is populated. Click on plus icon beside the student name for entering marks for additional subjects. "Update" against each student to compute the total. The mark list will be generated for the entire class. Click on "Publish" to send marks to all parents.', 'icon-paste3', './progresscard-generation', 3, 330d83af-dede-4d21-989f-500fe360b853, True, 'ST.PATRICKS ACADEMY HR. SEC. SCHOOL, DINDIGUL', True, True, 'Progress Card Generation', ['Employee', 'SchoolAdmin']);
INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (32aaa410-51d4-11e7-8dc1-3b9c08d048b3, f5a5e341-0700-4ed1-b591-76807ae1e4d1, 28428b95-fea3-4136-920c-603234f9753e, 1493828630000, '970ba5c4-62e6-4027-9662-ab320eb9fbbc/28428b95-fea3-4136-920c-603234f9753e/', False, 1525364630000, 3172122c-c09d-4b05-ad49-430aacb48a0d, 'Create Homework', 'Assignments and Homeworks can be created here for students and they can be notified of the same. Cllick on Add Assignment to create a new assignment. Assignments can either be published or saved as drafts. Drafts can later be edited and/or published. Assignments created can be found in the table below. Filters can be used to filter the necessary fields.', 'icon-reading', True, False, './create-assignments', 1, 970ba5c4-62e6-4027-9662-ab320eb9fbbc, True, 'ST.PATRICKS ACADEMY HR. SEC. SCHOOL, DINDIGUL', False, True, 'Create Homework', ['Employee', 'SchoolAdmin']);
INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (32aaa410-51d4-11e7-8dc1-3b9c08d048b3, f5a5e341-0700-4ed1-b591-76807ae1e4d1, 2a206b34-0a33-4d4c-8859-81df13c86feb, 1493828630000, '6f57fdf2-a978-4ebf-a111-31adeb578884/2a206b34-0a33-4d4c-8859-81df13c86feb/', False, 1525364630000, 31454062-d06e-446b-aeff-784d595a4213, 'Route Details', 'The route details of the school buses should be added here. Route details can be added by clicking on "Add Route". First, select the driver for the route and select the vehicle for the route. Next, provide the route details containing the point of origin and destination. Add a bus stop by providing the address or the latitude and longitude. Also provide the radius of distance within which when the bus enters, the notifications needs to be sent. Additional bus stops can be added by clicking on plus button on the right. Upon clicking "Next", map representation of the route is provided. On the next page, the conductor details can be added. All the routes that are added can be viewed in the table below.', 'icon-road', False, False, './route', 3, 6f57fdf2-a978-4ebf-a111-31adeb578884, True, 'ST.PATRICKS ACADEMY HR. SEC. SCHOOL, DINDIGUL', True, True, 'Route Details', ['Employee', 'SchoolAdmin']);
