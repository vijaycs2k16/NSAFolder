INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "help_text") VALUES (8eb846a0-4549-11e7-9543-276f818a8422, 42326603-6924-4dbb-a622-6126eada2cca, 8a7c5b0e-d98e-4573-8c5d-cb0c7e83349b, 'Written examinations in the system can be used for maintaining a consolidated list of written tests/exams conducted in school. Any new exams can be added by clicking on "Add Written Exam". Examinations can be scheduled by clicking on the next tab "Exam Schedule". By clicking on "Schedule Examination", exam timings for each subject can be specified along with maximum marks. Portions and syllabus for examinations can be added by moving to the next tab "Portions". Parents will be notified of all information after publishing the exam.');
