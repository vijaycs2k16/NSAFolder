INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (be3184d0-500e-11e7-9f5f-87d439384c1d, 6833e9cd-a70b-4a63-ab6c-0cdaef4a440c, d5692162-c80a-48bd-ae1f-be322196af7e, 1493828630000, '870e3768-9c7b-4d26-ba0c-c46d24a73ea6/d5692162-c80a-48bd-ae1f-be322196af7e/', False, 1525364630000, f54138b1-6f32-49bb-8f97-64515ddd1be9, 'Leave Assignment', 'Leaves can be assigned to employees here from the available leave types. The leaves can be assigned to employees by clicking on the "Assign Leaves" button,  and selecting the department for which the leave assignment is being done. Then select the reporting manager in the department and choose the employees who come under the reporting manager. When submitting the above, the employees and the reporting manager get associated. Then you can select the leaves that are applicable and click on "Assign". When an employee requests for a leave, the request is sent to the reporting manager who is associated here. The previously assigned leaves can be viewed in the table below.', 'icon-drag-right', False, False, './leave-assignments', 1, 870e3768-9c7b-4d26-ba0c-c46d24a73ea6, True, 'Auxilium English Medium School', True, True, 'Leave Assignment', ['Employee', 'SchoolAdmin']);
