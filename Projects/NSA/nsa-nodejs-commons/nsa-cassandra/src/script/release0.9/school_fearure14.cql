INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "notify_hostelers", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (8eb846a0-4549-11e7-9543-276f818a8422, 1535aa60-1ec6-4b99-848c-515fc2b63de8, 91dbe358-08eb-49ec-b864-cfda923c946c, 1493828630000, 'dd6d3d82-e5e5-440e-85ac-0681f19b07ba/91dbe358-08eb-49ec-b864-cfda923c946c/', False, 1525364630000, c34fe799-7266-4b75-95d4-16ac47f645e0, 'Timetable configuration', 'Timetable helps the teachers and students stay organised with the periods, exams, events and holidays. Teachers can view timetables for classes, their own schedule and swap classes if needed. Students and parents can be informed about class hours, upcoming exams and events with the help of notifications. Timetable configuration helps in defining the periods for classes and the respective timings for each of the periods. To create a timetable configuration, click on “Add configuration”. Provide the classes for which the configuration is applicable for. Provide the number of the periods to be a part of the configuration.
', 'icon-puzzle3', False, False, './timetable-configuration', False, 0, dd6d3d82-e5e5-440e-85ac-0681f19b07ba, True, 'GODSON PUBLIC SCHOOL.', True, True, 'Timetable configuration', ['Employee', 'SchoolAdmin']);
