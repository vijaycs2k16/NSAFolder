INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "notify_hostelers", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (e78a58d0-7bff-11e7-b2c8-3bbeadcdc431, 1987368a-9a9d-4fd8-aafb-c0aa3dc5b720, 02d33584-8a08-414d-ad87-72036813ac64, 1493828630000, '88d34e03-d9d0-431a-ae06-7a92ce2b4d3c/02d33584-8a08-414d-ad87-72036813ac64/', False, 1525364630000, 9876ac80-6410-440c-ad25-99d67be2c8fc, 'Assign Fees', 'Assign fees helps you to select the classes for which the fees is being assigned to and the specific launguages if the fees vary for each launguage. Specify the amount for each of the fee type and select if the deposits are fully or partially refundable along with the percentage refundable. Specify the due date for the fee payment and choose how to notify the parents about the fees. Once all of the information is entered and "Save" is clicked, the option to add scholarship is avaiable. Scholarship can be added by clicking on "Add Scholarship". Select the students eligible for the scholarship and the scholarship to be applied to their fees. Fee discount, if any, can also be specified here. Finally, a fee payment due date for the scholarship applicable students can be specified which can be different from the earlier mentioned one. Click Save after entering information. The fee will be published and the parents will be notified only after clcking on Publish.
', 'icon-credit-card2', True, False, './assign-fees', False, 2, 88d34e03-d9d0-431a-ae06-7a92ce2b4d3c, True, 'ST.PATRICKS ACADEMY HR. SEC. SCHOOL, DINDIGUL', True, True, 'Assign Fees', ['Employee', 'SchoolAdmin']);
