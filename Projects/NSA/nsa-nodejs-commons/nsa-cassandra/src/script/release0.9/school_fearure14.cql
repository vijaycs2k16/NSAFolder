INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (4cdbd690-24d1-11e7-9a7c-1108ffd2d102, 7ca3fad4-a571-495e-be1b-ac25de3a2009, 91dbe358-08eb-49ec-b864-cfda923c946c, 1493808830000, 'dd6d3d82-e5e5-440e-85ac-0681f19b07ba/91dbe358-08eb-49ec-b864-cfda923c946c/', False, 1525344830000, c34fe799-7266-4b75-95d4-16ac47f645e0, 'Timetable configuration', 'Timetable helps the teachers and students stay organised with the periods, exams, events and holidays. Teachers can view timetables for classes, their own schedule and swap classes if needed. Students and parents can be informed about class hours, upcoming exams and events with the help of notifications. Timetable configuration helps in defining the periods for classes and the respective timings for each of the periods. To create a timetable configuration, click on “Add configuration”. Provide the classes for which the configuration is applicable for. Provide the number of the periods to be a part of the configuration.
', 'icon-puzzle3', False, False, './timetable-configuration', 0, dd6d3d82-e5e5-440e-85ac-0681f19b07ba, True, 'BHASHYAM BLOOMS', True, True, 'Timetable configuration', ['Employee', 'SchoolAdmin']);
