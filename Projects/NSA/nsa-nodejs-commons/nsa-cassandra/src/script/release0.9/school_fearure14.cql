INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (ac8989d0-557c-11e7-a9da-618c0b66b59e, 68306a4b-a57c-418a-be07-f82ac044b5c5, 2a206b34-0a33-4d4c-8859-81df13c86feb, 1493828630000, '6f57fdf2-a978-4ebf-a111-31adeb578884/2a206b34-0a33-4d4c-8859-81df13c86feb/', False, 1525364630000, 31454062-d06e-446b-aeff-784d595a4213, 'Route Details', 'The route details of the school buses should be added here. Route details can be added by clicking on "Add Route". First, select the driver for the route and select the vehicle for the route. Next, provide the route details containing the point of origin and destination. Add a bus stop by providing the address or the latitude and longitude. Also provide the radius of distance within which when the bus enters, the notifications needs to be sent. Additional bus stops can be added by clicking on plus button on the right. Upon clicking "Next", map representation of the route is provided. On the next page, the conductor details can be added. All the routes that are added can be viewed in the table below.', 'icon-road', False, False, './route', 3, 6f57fdf2-a978-4ebf-a111-31adeb578884, True, 'Madrasathul Anwar  Higher Secondary School Kodinhi', True, True, 'Route Details', ['Employee', 'SchoolAdmin']);
