', 'icon-pencil-ruler', False, False, './assignement-types', 0, 970ba5c4-62e6-4027-9662-ab320eb9fbbc, True, 'BHASHYAM BLOOMS', True, True, 'Homework Types', ['Employee', 'SchoolAdmin']);
