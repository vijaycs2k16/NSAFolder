INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (32aaa410-51d4-11e7-8dc1-3b9c08d048b3, f5a5e341-0700-4ed1-b591-76807ae1e4d1, c4f852be-a6c0-41a1-8fcd-3b075e35a8b1, 1493828630000, 'a7337b14-90d1-4d88-bfb7-292b087ade36/c4f852be-a6c0-41a1-8fcd-3b075e35a8b1/', False, 1525364630000, 0f91ad31-2ded-4786-ac65-045eb77b7ec0, 'Section Allocation', 'The available sections can be allotted to each of the classes that have been provided. To associate sections with a class, click on Associate Section and choose the class for which the association is to be done. Choose the sections that are to be added under the particular class and provide the class strength. The allocations that are done can be found in the table below.', 'icon-tree5', False, False, './section-allocation', 1, a7337b14-90d1-4d88-bfb7-292b087ade36, True, 'ST.PATRICKS ACADEMY HR. SEC. SCHOOL, DINDIGUL', True, True, 'Section Allocation', ['Employee', 'SchoolAdmin']);
INSERT INTO "nsa"."school_feature" ("tenant_id", "school_id", "id", "activated_date", "asset_url", "email", "expire_date", "feature_id", "feature_name", "help_text", "icon", "is_channels", "is_override", "link", "order_by", "parent_feature_id", "push", "school_name", "sms", "status", "title", "user_types") VALUES (32aaa410-51d4-11e7-8dc1-3b9c08d048b3, f5a5e341-0700-4ed1-b591-76807ae1e4d1, c6f1b268-6083-48e2-8f5b-38739a1d6ad8, 1493828630000, '6f57fdf2-a978-4ebf-a111-31adeb578884/c6f1b268-6083-48e2-8f5b-38739a1d6ad8/', False, 1525364630000, 07c9a02a-82be-418f-aec3-a3a0af26cdda, 'Allocate Students', 'The students can be allocated to their respective routes here. Student allocation can be done by clicking on "Assign User". First select the route and the bus stop for allocation. Then select the class and section from which the students are to be allocated. By clicking on "Search" all the students from the selected class and section are listed below. For selecting all the students, click on the "Move All" button (Arrow button on left side). To add selected students, click on students to select and then click on "Move Selected" button (Button on right side). Students can be searched for using the search bar below the buttons. Individual students can also be added by dragging them and dropping under "Selected Students". Once the students are added under "Selected Students", click on "Assign". All the allocated students can viewed in the table below.', 'icon-user-plus', False, False, './allocated', 4, 6f57fdf2-a978-4ebf-a111-31adeb578884, True, 'ST.PATRICKS ACADEMY HR. SEC. SCHOOL, DINDIGUL', True, True, 'Allocate Students', ['Employee', 'SchoolAdmin']);
