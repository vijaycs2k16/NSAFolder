<% StudentsCollection.forEach(function(student, index){ %>
<tr data-index="<%=index + 1%>" data-id="<%=student._id%>">
    <td class="notForm centerCell">
        <label class="_customCHeckbox">
            <input type="checkbox" value="<%=student._id%>" class="checkbox"/>
            <span></span>
        </label>
    </td>
    <td class="text-left"><%=student.studentName%> <%=student.lastName%></td>
    <td class="notForm text-left"><%=student.studentEmail%></td>
    <td class="notForm text-left"><%=student.studentPhone%></td>
    <td class="notForm text-left"><%=student.center ? student.center.centerName : ''%></td>
    <td class="notForm text-left"><%=student.course ? student.course.courseName : '' %></td>
    <td><% if(student.studentStatus)  {%>
        <span data-color="bgGreenLight" class="text-right _tagSmall">Active</span>
        <% } else { %>
        <span data-color="bgRed" class="text-right _tagSmall">Deactive</span>
        <% } %></td>
    <td><%=student.createdBy%></td>

</tr>
<% }) %>

