<div class="form-holder _newForm">
    <form method="post" action="" id="invoiceForm">
        <h4 class="_dialogMainTitle">Edit Practice Exam</h4>
        <div class="_formWrap">
            <div class="_formBlock _equal">
                <div class="_animateInputBox">
                    <input type="text"
                           class="_animate "
                           id="name"
                           required
                           value="<%=model.name%>"/>
                    <label class="_animate">Name</label>
                </div>
            </div>
            <div class="_formBlock _equal">
                <div class="_animateInputBox _withInfo">
                    <input type="text"
                           name="examDuration"
                           class="_animate"
                           id="examDuration"
                           value="<%=model.duration%>"
                    />
                    <span class="_staticLabel">Duration (mins)</span>
                </div>
            </div>
        </div>
        <div class="_formWrap">
            <div class="_formBlock _equal">
                <div class="_animateInputBox">
                    <input type="number"
                           name="questionMarks"
                           class="_animate"
                           id="questionMarks"
                           value="<%=model.questionMark%>"
                    />
                    <span class="_staticLabel">Each Question Marks</span>
                </div>
            </div>
            <div class="_formBlock _equal">
                <div class="_animateInputBox">
                    <input type="number"
                           name="nagetiveMarks"
                           class="_animate"
                           id="nagetiveMarks"
                           value="<%=model.negativeMark%>"
                    />
                    <span class="_staticLabel">Negative Marks</span>
                </div>
            </div>
        </div>
        <div class="_formWrap _enlargedItemMargin newItem">
            <div class="_formBlock _equal">
                <div class="container">
                    <div class="canReadAndWrite row">
                        <div class="_circleRadioRadianceWrap col m-6">
                            <label class="_customCHeckbox">
                                <input type="checkbox"
                                       name="autoCorrect"
                                       checked=""
                                       class="checkbox"
                                       id="autoCorrect"
                                       value="<%=model.autoCorrect ? checked='checked' : ''%>"
                                />
                                <span class="clr-black">&nbsp;Auto Correct</span>
                            </label>
                        </div>
                        <div class="_circleRadioRadianceWrap col m-6" style="left: 10px">
                            <label class="_customCHeckbox">
                                <input type="checkbox"
                                       checked=""
                                       name="canReview"
                                       class="checkbox"
                                       id="canReview"
                                       value="<%=model.canReview ? checked='checked' : ''%>"
                                />
                                <span class="clr-black">&nbsp;Can Review</span>
                            </label>
                        </div>
                    </div>
                </div><br/><br/>
                <div class="_formWrap">
                    <div class="_formBlock _equal" style="">
                        <div class="canReadAndWrite row">
                            <div class="_animateSelectBox _withInfo col m-9" style="width: 420px; right: 25px">
                                <span class="_selectTitle margin-left-15">Course</span>
                                <div class="_newSelectListWrap">
                                    <a id="course"
                                       name="course"
                                       class="current-selected"
                                       data-id="<%=model.course ? model.course._id: null%>"
                                       href="javascript:;"><%=model.course ? model.course.courseName : 'Course Name'%></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="_formBlock _equal">
                        <div class="canReadAndWrite row">
                            <div class="_animateSelectBox _withInfo" style="width: 420px; left: 20px">
                                <span class="_selectTitle">Subject</span>
                                <div class="_newSelectListWrap">
                                    <a id="subject"
                                       name="subject"
                                       class="current-selected"
                                       data-id="<%=model.subject ? model.subject._id: null%>"
                                       href="javascript:;"><%=model.subject ? model.subject.subjectName : 'Subject Name'%></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="_formWrap">
                    <div class="_formBlock _equal">
                        <div class="canReadAndWrite row">
                            <div class="_animateSelectBox _withInfo" style="width: 420px; right: 10px">
                                <span class="_selectTitle">Topic</span>
                                <div class="_newSelectListWrap">
                                    <a id="topics"
                                       name="topics"
                                       class="current-selected"
                                       data-id="<%=model.topicData ? model.topicData._id: null%>"
                                       href="javascript:;"><%= model.topicData ? model.topicData.name : 'TopicData Name'%></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col m-6" style="margin-left: 15px;">
                        <div class="_animateInputBox">
                            <input type="number"
                                   class="_animate "
                                   id="questions"
                                   name="questions"
                                   maxlength="48"
                                   value="<%=model.questions ? model.questions.length: ''%>"/>
                            <label class="_animate">No Of Questions</label>
                        </div>
                    </div><br/><br/>
                </div><br/><br/>
                <div class="container" style="margin-left: 100px">
                    <div class="canReadAndWrite row">
                        <div class="_circleRadioRadianceWrap col m-4">
                            <div class="_tipsWrap">
                                <label for="manual" class="">Manual</label>
                                <input type="radio"
                                       name="questionType"
                                       id="manual"
                                       class="_circleRadioRadiance"
                                       value="ManualQuestions" <%= model.questionMode === 'ManualQuestions' ? checked ='checked' : '' %>
                                />
                                <label for="manual"
                                       class="_circleRadioRadianceLabel">
                                </label>
                            </div>
                        </div>
                        <div class="_circleRadioRadianceWrap col m-4">
                            <div class="_tipsWrap">
                                <label for="weightage" class="">Weightage</label>
                                <input type="radio"
                                       name="questionType"
                                       id="weightage"
                                       value="WeightageBase" <%= model.questionMode === 'WeightageBase' ? checked ='checked' : '' %>
                                       class="_circleRadioRadiance"/>
                                <label for="weightage"
                                       class="_circleRadioRadianceLabel">
                                </label>
                            </div>
                        </div>
                        <div class="_circleRadioRadianceWrap col m-4">
                            <div class="_tipsWrap">
                                <label for="random" class="">Random</label>
                                <input type="radio"
                                       name="questionType"
                                       id="random"
                                       value="Random" <%= model.questionMode === 'Random' ? checked ='checked' : '' %>
                                       class="_circleRadioRadiance"/>
                                <label for="random"
                                       class="_circleRadioRadianceLabel">
                                </label>
                            </div>
                        </div>
                    </div>
                </div><br/>
                <div class="container">
                    <div class="canReadAndWrite manualQuestions hide row">
                        <div class="col m-6">
                            <div class="_bundleListTitle">Questions List:</div>
                            <ul id='questionList' class="_productsForBundle"></ul>
                        </div>
                        <div class="col m-6">
                            <div class="_bundleListTitle">Select Questions:</div>
                            <ul id='productsBundle' class="_productsBundle"></ul>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="canReadAndWrite weightQuestions hide row">
                        <div class="inner-holder">
                            <div class="workflow-list-wrapper left">
                                <ul class="workflow-list"></ul>
                            </div>
                            <div id="workflowAccord" class="left">
                                <div id="workflowNames"></div>
                                <div id="details" class="workflow-details">
                                    <div id="workflowSubAccord" class="left">
                                        <div id="sub-details" class="workflow-details">
                                            <div class="listWorkflow">
                                                <div id="workflows"></div>
                                                <div id="addedStatus"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div><br/><br/>
                <div class="container">
                    <div class="canReadAndWrite generatedQuestions row">
                        <div class="col m-12">
                            <div class="_bundleListTitle">Questions</div>
                            <ul id='generatedQues' class="_productsForBundle"></ul>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="canReadAndWrite randomQuestions hide row">
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>