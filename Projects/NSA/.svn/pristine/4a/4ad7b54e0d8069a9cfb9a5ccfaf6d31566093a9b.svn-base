<input hidden id="depromoteSave" (click)="savePromoted($event)" >
<div id="DepromteStudents" class="overlay-popup footer-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <!-- <div class="hidden-xs simple-field-hf"></div> -->
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay($event)" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container">
                <form class="form-horizontal">
                    <div class="modal-body">
                        <legend class="text-semibold">
                            <i class="icon-list position-left"></i>
                             List Of Depromoted Students
                        </legend>
                        <table class="table datatable-depromote-list">
                            <thead>
                            <tr>
                                <th data-priority="1">Student Id</th>
                                <th>Student Name</th>
                                <th>Class & Section</th>
                                <th>Promoted To</th>
                                <th>Promote</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                    <div class="overlay-footer">
                        <div class="right-align overlay-footer-content-rt">
                            <button type="button" class="btn btn-default" (click)="closeOverlay($event)">Close</button>
                        </div>
                    </div>
                </form>
            </div>
            <!-- cd-panel-container -->
        </div>
    </div>
    <!-- right panel script -->
</div>
