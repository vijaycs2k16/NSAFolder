<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="nexrise.publication.in.nexrise.Attendence.ApprovalDetailActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height50"
            android:orientation="horizontal"
            android:weightSum="2">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:text="Employee Name : "
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:id="@+id/emp_name"
                android:text="Jhonson "
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height1"
            android:background="@color/overlayForegroundColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height50"
            android:orientation="horizontal"
            android:weightSum="2">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:text="Duration : "
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:id="@+id/duration"
                android:text="23 Oct - 27 Oct"
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height1"
            android:background="@color/overlayForegroundColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height50"
            android:orientation="horizontal"
            android:weightSum="2">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:text="Leave Type : "
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:id="@+id/leave_type"
                android:text="Sick Leave"
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height1"
            android:background="@color/overlayForegroundColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height50"
            android:orientation="horizontal"
            android:gravity="center"
            android:weightSum="2">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:text="Reason : "
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/common_text_size"
                android:padding="@dimen/padding10"
                android:id="@+id/reason"
                android:text="Not Feeling Well"
                android:textColor="@color/colorBlack"
                android:layout_gravity="center"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height1"
            android:background="@color/overlayForegroundColor"/>
    </LinearLayout>
    <ExpandableListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_status_listview"
        android:groupIndicator="@null"
        android:background="@color/layoutBackground"/>

</LinearLayout>
