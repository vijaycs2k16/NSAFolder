<% collection.forEach(function(item, index){ %>
<tr data-id="<%=item._id%>">
    <td data-content="name" class="editable text-left"><%=item.school_name ? item.school_name : '-'%></td>
    <td style="text-align: left">
        <a href="javascript:;" class="icon-note goToEdit _actionCircleBtn show select-disabled" aria-hidden="true"></a>
        <a href="javascript:;" class="icon-trash goToRemove _actionCircleBtn show" aria-hidden="true" ></a>
    </td>
</tr>
<% }) %>

