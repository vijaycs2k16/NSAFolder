<div class="_acountingTableWrap">
    <div class="_acountingTitleBlock _forOverview">
        <a href="javascript:;" class="btn blue slim icon-plus btnPositioned" title="Create" id="top-bar-createBtn">New</a>
    </div>
    <div class="_acountingTableBlock">
        <span>You can change Base Currency in <a class="_link" href="#erp/organizationSettings">Organization Profile</a></span>
        <table class="list tableProjects tableFixed _currencyTable">
            <colgroup>
                <col width="40%"/>
                <col width="40%"/>
                <col width="20%"/>
                <col width="20%"/>
            </colgroup>
            <thead>
            <tr class="forToggle">
                <th class="text-left">Name</th>
                <th class="text-left">Symbol</th>
                <th class="text-left">Active</th>
                <th></th>
            </tr>
            </thead>
            <tbody class="forToggle" id="currencyTable">
            <% collection.forEach(function(elem,index){ %>

            <tr data-id="<%=elem._id%>" class="<%=elem.active ? 'gery' : ''%>">
                <% if (App && App.organizationSettings && elem._id === App.organizationSettings.currency._id){ %>
                <td class="_baseCurrency text-left"><span><%=elem.name%></span>
                    <div>(Base Currency)</div>
                </td>
                <td class="text-left"><span><%=elem.symbol%></span></td>
                <td class="text-left"><span><%=elem.active ? 'Yes' : 'No'%></span></td>
                <td class="text-left">
                    <a href="javascript:;" class="icon-note goToEdit _actionCircleBtn" aria-hidden="true"></a>
                </td>
                <% } else { %>
                <td class="text-left"><span><%=elem.name%></span></td>
                <td class="text-left"><span><%=elem.symbol%></span></td>
                <td class="text-left"><span><%=elem.active ? 'Yes' : 'No'%></span></td>
                <td class="text-left">
                    <a href="javascript:;" class="icon-note goToEdit _actionCircleBtn" aria-hidden="true"></a>
                    <a href="javascript:;" class="icon-trash goToRemove _actionCircleBtn" aria-hidden="true"></a>
                </td>
                <% }%>
            </tr>

            <% }) %>
            </tbody>
        </table>
    </div>
</div>