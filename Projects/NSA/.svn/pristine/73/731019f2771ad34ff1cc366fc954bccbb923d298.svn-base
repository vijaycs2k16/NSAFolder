<hr/>

<div class="panel-heading">
    <h6 class="panel-title"><span class="text-semibold text-uppercase"><i
            class="icon-clipboard position-left"></i>List of Assessments</span></h6>
    <div class="heading-elements">
        <input type="button" value="Add Assessment Type" data-size="50%" data-title="Add Assessment Type"
               data-toggle="from-right" data-id="#add_assessment" class="btn btn-sm btn-primary heading-btn"
               (click)="addAssessment($event)"/>
    </div>
</div>
<hr/>
<div class="panel-body">
    <table class="table datatable-export">
        <thead>
        <tr>
            <th>S.No</th>
            <th>Name</th>
            <th>Code</th>
            <th>Updated By</th>
            <th>Action</th>
        </tr>
        </thead>

    </table>
</div>
