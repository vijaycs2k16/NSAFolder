<div id="mark-upload-config" class="overlay-popup footer-popup" style="display: none;">
    <div class="cd-panel">
        <div class="cd-panel-header">
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay()" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container">
                <div class="panel panel-default" [hidden]="edit">
                    <div class="filter-panel-body" style="height:70px">
                        <form class="form-horizontal">
                            <div class="form-group col-md-3">
                                <label class="control-label col-md-3">Class <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #classSelect id="class-name" class="select-change" (click)="getSectionByClass()" data-width="100%">
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div *ngIf="!enable" class="form-group col-md-4" [hidden]="!classId">
                                <label class="control-label col-md-3">Section <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #sectionSelect id="section-name" data-width="100%" class="select-change" (click)="getExamsByClassAndSections()">
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div *ngIf="enable" class="form-group col-md-4" [hidden]="!classId">
                                <label class="control-label col-md-3">Section <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #sectionSelect id="section-name1" data-width="100%" class="select-change" (click)="getTermsByClassAndSections()">
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div *ngIf="enable" class="form-group col-md-4" [hidden]="!sectionId">
                                <label class="control-label col-md-3">Term <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #termSelect id="exam-term" class="select-change" (click)="getExamsByClassAndSections1()" data-width="100%">
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div *ngIf="!enable" class="form-group col-md-4" [hidden]="!classId || !sectionId">
                                <label class="control-label col-md-3">Exam <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <!--<select #examSelect id="exam-name" class="select-change" (click)="getUsersByClassAndSection()" data-width="90%">-->
                                        <select #examSelect id="exam-name" (click)="getExamId()"  class="select-change" data-width="90%">
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div *ngIf="enable" class="form-group col-md-4" [hidden]="!classId || !termId">
                                <label class="control-label col-md-3">Exam <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <!--<select #examSelect id="exam-name1" class="select-change" (click)="getUsersByClassAndSection()" data-width="90%">-->
                                        <select #examSelect id="exam-name1" (click)="getExamId()" class="select-change" data-width="90%">
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2" [hidden]="!examId">
                                <div class="form-group">
                                    <!--<input class="btn btn-default " id="cancel-back" value="Clear" (click)="resetForm()" type="reset">-->
                                    <!--<input class="btn btn btn-primary ui-wizard-content margin-right10 btn-xs  btn-ladda btn-ladda-spinner ui-formwizard-button" data-style="zoom-out" data-spinner-size="20" id="search" value="Search" (click)="getUsersByClassAndSection()"   type="button">-->
                                    <button type="button" id="search" class="btn btn-primary btn-xs btn-ladda btn-ladda-spinner update-mark" data-size="xs" data-style="zoom-out" data-spinner-size="20" (click)="getUsersByClassAndSection()"><span class="ladda-label">Search</span></button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="panel panel-default" *ngIf="publish">
                    <div class="filter-panel-body">
                        <form class="form-horizontal">
                            <div class="form-group col-md-6">
                                <label class="control-label col-md-2">Channels
                                </label>
                                <label class="checkbox-inline sms hide" style="font-size:12px;">
                                    <input  #sms type="checkbox" class="smsChecked"  checked="checked">SMS
                                </label>
                                <label class="checkbox-inline email hide" style="font-size:12px;">
                                    <input #email type="checkbox" class="emailChecked">Email
                                </label>
                                <label class="checkbox-inline push hide" style="font-size:12px;">
                                    <input #push type="checkbox" class="pushChecked">App
                                </label>
                            </div>
                        </form>
                    </div>
                </div>
                <input type="hidden" id="upload-mark" (click)="uploadMark($event)">
                <input type="hidden" id="upload-mark1" (click)="uploadMark1($event)">

                <div class="panel-body ">
                    <div *ngIf="enable" class="modal-body" [hidden]="!classId || !sectionId || !exam_Id">
                        <div class="tabbable ">
                            <ul class="nav nav-tabs nsa-nav-tabs nsa-blue nav-tabs-highlight">
                                <li class="active"><a href="#academic" data-toggle="tab"> Academic </a></li>
                                <li><a href="#non_academic" data-toggle="tab">Non Academic</a></li>
                            </ul>
                                <div  class="tab-content">
                                      <div class ="tab-pane active" id="academic" >
                                        <div class="panel panel-default">
                                            <!--<div class="panel-heading">-->
                                                <!--<h5 class="panel-title">Upload Marks</h5>-->
                                            <!--</div>-->
                                        </div>
                                        <div class ="tab-pane active ">
                                            <div class="panel-body">
                                                <table id="example1" class="table display mark-upload" width="100%"></table>
                                            </div>
                                        </div>
                                        </div>

                                    <div id="non_academic" class="tab-pane">
                                        <div class="panel-body">
                                            <table id="nonacademic" class="table display mark-upload1" width="100%"></table>
                                        </div>
                                    </div>
                                </div>
                        </div>
                    </div>

                    <div *ngIf="!enable" class="panel panel-default" [hidden]="!classId || !sectionId || !exam_Id">
                        <div class="panel-heading">
                            <h5 class="panel-title">Upload Marks</h5>
                        </div>

                        <div class="panel-body">
                            <table id="example" class="table display mark-upload" width="100%"></table>
                        </div>
                    </div>
                </div>


                <!--<div *ngIf="!enable"  class="modal-body" [hidden]="!classId || !sectionId || !examId">-->
                    <!--<div class="panel panel-default">-->
                        <!--<div class="panel-heading">-->
                            <!--<h5 class="panel-title">Upload Marks</h5>-->
                        <!--</div>-->

                        <!--<div class="panel-body">-->
                            <!--<table id="example" class="table display mark-upload" width="100%"></table>-->
                        <!--</div>-->
                    <!--</div>-->
                <!--</div>-->


            </div>
            <hr/>
            <div class="overlay-footer">
                <div class="right-align overlay-footer-content-rt">
                    <button type="button" (click)="closeOverlay()" class="btn btn-default">Close</button>
                    <button *ngIf="publish" type="submit" id="publishButton" (click)="publisMarkList($event)" class="btn btn-primary btn-ladda btn-ladda-progress" data-style="expand-right" data-spinner-size="20"><span class="ladda-label">Publish</span></button>
                </div>
            </div>
        </div>
    </div>
</div>