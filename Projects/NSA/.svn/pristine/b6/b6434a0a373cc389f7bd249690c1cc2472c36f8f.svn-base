<div id="subjectAllocation" class="overlay-popup footer-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <!-- <div class="hidden-xs simple-field-hf"></div> -->
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay($event)" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container" id="hiflame-cart-popup">
            <div class="cd-panel-content" id="side-panel-container">
                    <div class="modal-body">
                        <form action="#" class="form-horizontal from-validate" [formGroup]="subjectAllocationform">
                            <div class="form-group ">
                                <label class="control-label col-md-4">Class  <span class="text-danger">*</span></label>
                                <div class="col-md-8">
                                    <p-tree  [value]="data">
                                        <template let-node  pTemplate type="default">
                                            <input type="hidden" formControlName="classes">
                                            <input type="checkbox" [(ngModel)]="node.selected" [ngModelOptions]="{standalone: true}" [indeterminate]="node.partialSelection" (ngModelChange)="nodeSelected(node)"><span style="margin-left: 6px">{{node.label}}</span>
                                        </template>
                                    </p-tree>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-4">Subject Name <span class="text-danger">*</span> </label>
                                <div class="col-md-8">
                                    <div class="multi-select-full">
                                        <select #selectAll class="multiselect-subject" multiple="multiple" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group ">
                                <label class="control-label col-md-4">Subject Type  <span class="text-danger">*</span></label>
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label class="radio-inline">
                                            <input  type="radio"  name="subjectType" formControlName="subjectType" (click)="checkClass('true')" class="styled radio1" [value]="true"  >Academic
                                        </label>
                                        <label class="radio-inline">
                                            <input  type="radio" name="subjectType" formControlName="subjectType" (click)="checkClass('false')"  class="styled radio2" [value]="false">Non-Academic
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <hr/>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" (click)="closeOverlay($event)">Close</button>
                                <button type="submit" id="save" class="btn btn-primary btn-ladda btn-ladda-progres" (click)="saveSubjectAllocation('save')">{{btnVal}}</button>

                            </div>
                        </form>
                    </div>
            </div>
            <!-- cd-panel-container -->
        </div>
    </div>
    <!-- right panel script -->
</div>

