<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/events_feature_relative"
    tools:context="nexrise.publication.in.nexrise.EventsFeature.EventsFeatureFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/overlayForegroundColor"
        android:id="@+id/event_feature_linear"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="3">
            <TextView
                android:text="ALL ACTIVITIES"
                android:layout_width="0dp"
                android:layout_height="@dimen/height50"
                android:id="@+id/textView16"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding15"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/headings"
                android:layout_weight="2" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_marginTop="@dimen/margin10"
                android:background="@color/overlayForegroundColor"
                android:layout_weight="1">

                <ImageButton
                    android:layout_width="@dimen/height40"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_more_menu"
                    android:background="#d7d3d3"
                    android:id="@+id/imageButton"
                    />

                <ImageButton
                    android:layout_width="@dimen/height40"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_grid_layout"
                    android:id="@+id/imageButton2"
                    android:background="#d7d3d3"
                    android:layout_marginLeft="@dimen/margin3"
                    />
                <ImageView
                    android:layout_weight="0.5"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_help"
                    android:layout_width="0dp"
                    android:id="@+id/help"
                    android:layout_height="@dimen/height20" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height50"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:baselineAligned="false"
        android:id="@+id/footer"
        android:weightSum="5"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="2"
            android:id="@+id/all_activiies_footer"
            android:background="@color/appColor"
            android:layout_weight="1">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/margin5"
                app:srcCompat="@drawable/ic_all_activities"
                android:layout_weight="1"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="All"
                android:textSize="@dimen/common_text_size"
                android:paddingLeft="@dimen/padding5"
                android:paddingRight="@dimen/padding5"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/colorWhite"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="2"
            android:id="@+id/first_footer"
            android:background="@color/appColor"
            android:layout_weight="1">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/margin5"
                app:srcCompat="@drawable/ic_footer_events"
                android:layout_weight="1"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="Events"
                android:textSize="@dimen/common_text_size"
                android:paddingLeft="@dimen/padding5"
                android:paddingRight="@dimen/padding5"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/colorWhite"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="2"
            android:id="@+id/second_footer"
            android:background="@color/appColor"
            android:layout_weight="1">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/margin5"
                app:srcCompat="@drawable/ic_competitions"
                android:layout_weight="1"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="Comp."
                android:textSize="@dimen/common_text_size"
                android:paddingLeft="@dimen/padding5"
                android:paddingRight="@dimen/padding5"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/colorWhite"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="2"
            android:id="@+id/third_footer"
            android:background="@color/appColor"
            android:layout_weight="1">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/margin5"
                app:srcCompat="@drawable/ic_ecc"
                android:layout_weight="1"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="EC"
                android:textSize="@dimen/common_text_size"
                android:paddingLeft="@dimen/padding5"
                android:paddingRight="@dimen/padding5"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/colorWhite"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="2"
            android:id="@+id/fourth_footer"
            android:background="@color/appColor"
            android:layout_weight="1">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/margin5"
                app:srcCompat="@drawable/ic_cc"
                android:layout_weight="1"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="SEA"
                android:textSize="@dimen/common_text_size"
                android:paddingLeft="@dimen/padding5"
                android:paddingRight="@dimen/padding5"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/colorWhite"/>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_below="@+id/event_feature_linear"
        android:layout_marginBottom="@dimen/margin5"
        android:background="@color/backgroundColor"
        android:id="@+id/events_feature_frame"
        android:layout_above="@id/footer">

    </RelativeLayout>
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black_transparent"
        android:visibility="gone"
        android:id="@+id/whiteback"
        android:gravity="center_vertical">
    </RelativeLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/fab"
        android:layout_above="@id/footer"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2"
            android:id="@+id/add_events_with_text">
            <TextView
                android:layout_width="@dimen/width116"
                android:layout_height="wrap_content"
                android:text="Create Activity"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/headings"
                android:textStyle="bold"
                android:gravity="right"
                android:layout_marginRight="@dimen/margin5"
                android:layout_gravity="center"
                android:layout_weight="1" />
        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:elevation="2dp"
            app:fabSize="normal"
            android:background="@color/appColor"
            app:backgroundTint="@color/appColor"
            android:tint="@color/colorWhite"
            app:useCompatPadding="true"
            app:srcCompat="@drawable/ic_input_add1"
            android:id="@+id/add_events"
            android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/past_events_with_text">
            <TextView
                android:layout_width="@dimen/width116"
                android:layout_height="wrap_content"
                android:text="Past Activities"
                android:id="@+id/past_event_textview"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/headings"
                android:textStyle="bold"
                android:gravity="right"
                android:layout_marginRight="@dimen/margin5"
                android:layout_gravity="center" />
            <android.support.design.widget.FloatingActionButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:fabSize="normal"
                android:background="@color/appColor"
                app:backgroundTint="@color/appColor"
                app:useCompatPadding="true"
                android:tint="@color/colorWhite"
                app:elevation="4dp"
                app:srcCompat="@drawable/ic_pastevents"
                android:id="@+id/past_events"/>
        </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        android:id="@+id/more_events_with_text">
        <TextView
            android:layout_width="@dimen/width116"
            android:layout_height="wrap_content"
            android:text=""
            android:gravity="right"
            android:layout_marginRight="@dimen/margin5"
            android:textColor="@color/colorWhite"
            android:textSize="@dimen/description_text_size"
            android:layout_gravity="center" />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_events_feature"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/appColor"
        app:backgroundTint="@color/appColor"
        android:tint="@color/colorWhite"
        android:onClick="runThis"
        app:elevation="2dp"
        app:srcCompat="@drawable/ic_vertical_dots"
        app:useCompatPadding="true"
        app:fabSize="normal"/>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>
