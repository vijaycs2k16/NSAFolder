                    <div class="form-group">
                        <label class="control-label col-md-3">Choose Role Types&nbsp;<span class="text-danger">*</span></label>
                        <div class="col-md-9">
                            <div class="multi-select-full">
                                <select #selectRoleypes class="select-role-types" multiple="multiple">
                                </select>
                            </div>
                        </div>
                    </div>
