<div id="allocateRole" class="overlay-popup footer-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay($event)" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container" id="hiflame-cart-popup">
            <div class="cd-panel-content" id="side-panel-container">
                <form action="#" class="form-horizontal form-multiselector">
                    <div class="modal-body">
                        <div class="form-group">
                            <label class="control-label col-md-3">Role Types&nbsp;<span class="text-danger">*</span></label>
                            <div class="col-md-9">
                                <div class="multi-select-full">
                                    <select #selectRole id="select-role" class="select-change" (click)="search()" data-width="100%">
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading margin-bottom20">
                                <h6 class="panel-title second-panel-title">
                                    <span class="text-semibold text-uppercase">
                                        <i class = "icon-menu7 position-left"></i>
                                        Select Employees
                                    </span>
                                </h6>
                            </div>

                            <div class="panel-body">
                                <div class="col-md-12">
                                    <div class="row">
                                        <div class="col-md-6 text-center text-semibold text-uppercase">Available Employees</div>
                                        <div class="col-md-6 text-center text-semibold text-uppercase">Selected Employees </div>
                                    </div>
                                    <dual-list [source]="emp" [(destination)]="selectedEmp" (destinationChange)="true" [key]="key" [display]="display" [filter]="true" [format]="format"></dual-list>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
