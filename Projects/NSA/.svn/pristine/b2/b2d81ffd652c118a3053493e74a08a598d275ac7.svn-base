<div id="HallofFameView" class="overlay-popup footer-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay()" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container">
                <form action="#" class="form-horizontal">
                <div class="modal-body">
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label col-sm-4 txt-align-left">Award Name&nbsp;<span class="text-danger">*</span></label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" [ngModelOptions]="{standalone: true}"  [(ngModel)]="awardName" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label col-sm-4 txt-align-left">Date of Issue&nbsp;<span class="text-danger">*</span></label>
                            <div class="col-sm-8">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="icon-calendar22"></i></span>
                                    <input type="text" class="form-control" [ngModelOptions]="{standalone: true}"  [(ngModel)]="dateOfIssue"  readonly>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label col-sm-4 txt-align-left">Category</label>
                            <div class="col-sm-8">
                                <textarea rows="3" cols="3" class="form-control" [ngModelOptions]="{standalone: true}" [(ngModel)]="cartegory" readonly></textarea><br/>
                            </div>
                        </div>
                    </div>
                    <div class="panel-body">
                        <table class="table datatable-view-hall-of-fame">
                            <thead>
                            <tr>
                                <th data-priority="1">Student Name</th>
                                <th>Class</th>
                                <th>Section</th>
                                <th data-priority="2">Status</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
                <div class="overlay-footer">
                    <div class="right-align overlay-footer-content-rt">
                        <button type="button" class="btn btn-default" (click)="closeOverlay()">Close</button>
                    </div>
                </div>
                </form>
            </div>
            <!-- cd-panel-container -->
        </div>
    </div>
    <!-- right panel script -->
</div>