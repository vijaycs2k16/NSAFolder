<div class="panel panel-default">
    <div class="panel-heading">
        <h6 class="panel-title"><span class="text-semibold text-uppercase"><i class="fa fa-users position-left"></i>Students</span><i class="icon-arrow-right15 text-pink-600"></i> <small>List of All Students </small></h6>
        <div class="heading-elements text-right" style="right: 275px; width: 22%">
            <div class="col-sm-8">
                <label class="control-label col-sm-4 text-right" style="top: 7px;"> Filter :</label>
                <div class="col-sm-8">
                    <div class="row">
                        <select  #singleFilter id="bootstrap-Filter" (click)="getStudentByFilter($event, 'active')" class="select-change" data-width="75%">
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <div class="heading-elements">
            <!--<button class="btn btn-sm btn-primary heading-btn" data-toggle="modal" data-target="#modal_theme_primary1"><i class="fa fa-upload position-left"></i>Import</button>-->
            <input type="button" value="Add Student" data-size="75%" data-title="Add Student" data-toggle="from-right" data-id="#addStudent" class="btn btn-sm btn-primary heading-btn" (click)="addStudent($event)"/>
            <input type="button" value="Student Report " data-size="75%" data-title="Student Report" data-toggle="from-right" data-id="#reportStudent" class="btn btn-sm btn-primary heading-btn postion-left" (click)="reportStudent($event)"/>
            <input type="button" *ngIf="enable" value="Student TC Report" data-size="100%" data-title="Student TC Report" data-toggle="from-right" data-id="#reporttcStudent" class="btn btn-sm btn-primary heading-btn" (click)="reporttcStudent($event)"/>
        </div>
    </div>
    <div class="panel-body">
        <input hidden id="deleteStudent" (click)="deleteStudent($event)" type="button" style="display: none;">
        <input hidden id="resetWarning" (click)="request_warning()" type="button" style="display: none;">
        <input hidden id="resetPwd" (click)="resetPwd($event)" type="button">
        <input hidden id="checkRoleToResetPwd" (click)="checkRoleToResetPwd($event)" type="button">
        <input type="hidden" id="deleteButton" (click)="confirmDelete()" />
        <input type="hidden" id="reload" (click)="reload()" />
        <input hidden id="editStudent" (click)="editStudent($event)" data-title="Edit Student" data-size="75%" data-id="#addStudent" type="button" style="display: none;">
        <input hidden id="printStudent" (click)="printStudent($event)" data-title="Edit Student" data-size="75%" data-id="#addStudent" type="button" style="display: none;">

        <!--<label class="control-label col-sm-1" style="top:45px; left:342px" > Filter :</label>-->
        <!--<div class="col-sm-2 selection"  style="left:290px; top:37px;     cursor: default; position: relative; clear: none !important;">-->
            <!--<div class="row">-->
                <!--<select  #singleFilter id="bootstrap-Filter" (click)="getStudentByFilter($event)" class="select-change " data-width="75%">-->
                <!--</select>-->
            <!--</div>-->
        <!--</div>-->


        <table class="table datatable-student">
            <thead>
            <tr>
                <th data-priority="1">Admn No.</th>
                <th>Roll No</th>
                <th>Student ID</th>
                <th width="20%">Name</th>
                <th>Class</th>
                <th>Section</th>
                <th>Phone Number</th>
                <th data-priority="3">Status</th>
                <th>Reset Password</th>
                <th width="10%"  data-priority="2">Action</th>
            </tr>
            </thead>
            <tfoot>
            <tr>
                <th>Admission No</th>
                <th>Roll No</th>
                <th>Student ID</th>
                <th width="20%">Name</th>
                <th>Class</th>
                <th>Section</th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
            </tr>
            </tfoot>
        </table>
    </div>
</div>
<add-student></add-student>
<student-report></student-report>
<student-tc-report></student-tc-report>
<div id="modal_theme_primary1" class="modal fade refuse">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header bg-info">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h5 class="modal-title">Import Employee</h5>
            </div>
            <div class="modal-body">
                <form class="form-horizontal form-validate">
                    <div class="form-group">
                        <label class="col-lg-2 control-label text-semibold">Select File</label>
                        <div class="col-lg-10">
                            <input type="file" class="file-input">
                        </div>
                    </div>
                </form>
                <div class="form-group">
                    <div class="panel border-left-lg border-left-primary">
                        <div class="panel-body alpha-blue">
                            <div class="row">
                                <div class="col-md-8">
                                    <h6 class="no-margin-top text-semibold">General Instructions</h6>
                                    <ul>
                                        <li>The Field with red color are mandatory</li>
                                        <li>All date must be enter in DD/MM/YYYY format</li>
                                        <li>Birth date must be less than curren date</li>
                                        <li>E-mail id and phone number shoulld be in valid format</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="form-group">
                                <button class="btn btn-sm btn-primary heading-btn" data-toggle="modal" data-target="#modal_theme_primary1"><i class="fa fa-download position-left"></i>Template</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-sm btn-default" data-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</div>