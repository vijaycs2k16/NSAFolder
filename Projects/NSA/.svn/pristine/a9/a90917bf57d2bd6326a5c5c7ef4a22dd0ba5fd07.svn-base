<table class="list _projectTableLeftAlign tableFixed _invoiceTable">
    <colgroup>
        <col width="100px">
        <!--<col width="200px">-->
        <!--<col width="10%">-->
        <col width="100px">
        <col width="100px">
        <col width="100px">
        <col width="100px">
    </colgroup>
    <thead>
    <tr class="productListItemsColumns _deleteHeader">
        <th class="text-left">
            SKU
        </th>
        <th class="text-left">
            Product
        </th>
        <th class="width20">
            Ordered
        </th>
        <th class="width20">
            Receive now
        </th>
        <th class="width20">
            Location
        </th>
    </tr>
    </thead>
    <tbody id="tbodyProducts">
    <tr></tr>

    <%products.forEach(function(elem, i){%>

    <%if (elem.product) {%>
    <tr class="productItem deleteRow" id="<%=elem._id%>" data-id="<%=elem.warehouse ? elem.warehouse._id : ''%>">

        <td data-name="productName">

            <a class="current-selected" id="productsDd" href="javascript:;"
               data-id="<%= elem.product ? elem.product._id : ''%>">

                <%=elem.product && elem.product.info ? elem.product.info.SKU : 'Select' %>
            </a>


        </td>
        <td data-name="description">
            <%= elem.product ? elem.product.name : ''%>
        </td>
        <td data-name="ordered">
            <input id="ordered" class="statusInfo" style="padding-right: 25px;" maxlength="9" value="<%=elem.quantity%>" size=7 type="text" readonly/>
        </td>
        <td data-name="newShip">
            <div class="receiveLocation" style="padding-left: 125px;">
                <div>
                    <input id="newShip" class="quantity" maxlength="9" value="<%=elem.quantity%>"
                           size=7 type="text"/>
                </div>
                <!--<div style="padding-left: 210px;">
                    <a class="current-selected locationDd" id="locationDd" style="bottom: 15px" href="javascript:;"
                       data-id="<%= elem.locationsDeliver ? elem.locationsDeliver[0]._id : ''%>">
                        <%=elem.locationsDeliver ? elem.locationsDeliver[0].name: 'Select' %>
                    </a>
                </div>-->
            </div>

        </td>
        <td data-name="location" class="text-left">
            <div class="_animateSelectBox">
                <div class="_newSelectListWrap" style="width: 85px; left: 105px;">
                    <a class="current-selected"
                       id="locationDd"
                       href="javascript:;"
                       data-id="<%= elem.locationsDeliver ? elem.locationsDeliver[0]._id : ''%>">

                        <%=elem.locationsDeliver ? elem.locationsDeliver[0].name: 'Select' %>
                    </a>
                </div>
            </div>
        </td>
    </tr>
    <%}%>
    <%})%>

    <tr>
        <td class="invisible_border">&nbsp;</td>
    </tr>
    </tbody>
</table>
<div class="clearfix"></div>
<div id="totalAmountContainer" class="_totalAmountContainer _createPopup">

</div>