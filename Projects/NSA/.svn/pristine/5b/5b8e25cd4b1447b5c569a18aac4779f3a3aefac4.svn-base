<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent">




    <android.support.v7.widget.CardView
        android:layout_width="0dp"
        android:layout_height="@dimen/height50"
        android:layout_weight="2"
        app:cardElevation="@dimen/cardElevation3"
        android:layout_marginBottom="@dimen/margin5"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardBackgroundColor="@color/layoutBackground"
        app:cardUseCompatPadding="true">


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin5"
            android:gravity="center_vertical"
            android:weightSum="3">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/height50"
                app:srcCompat="@drawable/ic_clip"
                android:id="@+id/imageView9"
                />
            <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="@dimen/height50"
                android:id="@+id/drawer_textview"
                android:textColor="@color/colorBlack"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/margin15"
                android:paddingTop="@dimen/margin5"
                android:paddingBottom="@dimen/margin5"
                android:layout_weight="2"
                android:textSize="@dimen/description_text_size"/>

            <ImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/ic_download"
                android:id="@+id/imageView10"
                android:layout_weight="1" />
        </LinearLayout>
    </android.support.v7.widget.CardView>
</LinearLayout>