<div id="edit_timetable" class="overlay-popup footer-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <!-- <div class="hidden-xs simple-field-hf"></div> -->
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay($event)" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container" id="hiflame-cart-popup ">
            <div class="cd-panel-content" id="side-panel-container">
                <div class="panel-default panel-body-bottom-padding">
                    <!-- Working Days -->

                    <div class="panel-heading">
                        <h6 class="panel-title second-panel-title"><span class="text-semibold text-uppercase"><i
                                class="icon-clipboard2 position-left"></i>Working Days</span></h6>
                    </div>
                    <form action="#" class="form-horizontal" [formGroup]="editTimetableconfigForm" [ngClass]="{'not-active': !enableData}">

                        <div class="panel-body">
                            <div class="form-group col-md-12">
                                <div #daysSelect class="checkbox col-md-12">
                                    <div  *ngFor="let day of days">
                                        <label *ngIf="day.id != 0" class="col-md-2">
                                            <input type='checkbox' class="{{day.id}}" value="{{day.id}}">{{day.name}}
                                        </label>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="panel-default panel-body-bottom-padding">
                            <div class="panel-heading">
                                <h6 class="panel-title second-panel-title"><span class="text-semibold text-uppercase"><i
                                        class="fa fa-clock-o position-left"></i>Class timings</span></h6>
                            </div>
                            <div class="page-header page-header-content">
                                <div class="page-title padding-bottom0">
                                    <fieldset class="ng-untouched ng-pristine ng-valid">
                                        <div class="form-group">
                                            <label class="control-label col-md-2">Regular School Hours <span
                                                    class="text-danger">*</span></label>

                                            <div #pickerStartTime class="col-md-3">
                                                <datetime  [datepicker]="false" [timepicker]="{ showMeridian: true, minuteStep: 1, showSeconds: false }"  [ngModelOptions]="{standalone: true}" [(ngModel)]="date2"></datetime>
                                            </div>
                                            <div #pickerEndTime class="col-md-3">
                                                <datetime [datepicker]="false" [timepicker]="{ showMeridian: true, minuteStep: 1, showSeconds: false}"  [ngModelOptions]="{standalone: true}" [(ngModel)]="date3"></datetime>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-2">No.of Periods (including breaks) <span
                                                    class="text-danger">*</span></label>
                                            <div class="col-md-3">
                                                <select #singleEditPeriods (click)="generatePeriod()" id="bootstrap-edit-periods" class="form-control bootstrap-select select-change">
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-sm-2">Class Hours</label>
                                            <div class="col-md-10 ">
                                                <div class="panel panel-flat table-responsive">
                                                    <table class="table">
                                                        <thead>
                                                        <tr>
                                                            <th>Period</th>
                                                            <th> Start Time </th>
                                                            <th> End Time </th>
                                                            <th>Break Time</th>
                                                        </tr>
                                                        </thead>
                                                        <tbody #line>
                                                        <tr *ngFor="let row  of data; let i=index">
                                                            <td><select #schoolPeriod id="edit{{i}}"  class="form-control"></select></td>
                                                            <td *ngIf="i != -1"><datetime [datepicker]="false" [timepicker]="{ showMeridian: true, minuteStep: 1, placeholder: o}"  [ngModelOptions]="{standalone: true}" [(ngModel)]="date[i]"></datetime></td>
                                                            <td *ngIf="i != -1"><datetime id="date-{{i}}" [datepicker]="false" [timepicker]="{ showMeridian: true, minuteStep: 1 }"[ngModelOptions]="{standalone: true}" [(ngModel)]="date1[i]"></datetime></td>
                                                            <td>  <input id="checkbox{{i}}" type="checkbox"></td>
                                                        </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>

                            <div class="page-header page-header-content">
                                <div class="page-title padding-bottom0">
                                    <fieldset class="ng-untouched ng-pristine ng-valid">

                                    </fieldset>
                                </div>
                            </div>
                            <!-- Class hour name -->
                            <div class="page-header page-header-content">
                                <div class="page-title padding-bottom0">
                                    <fieldset class="ng-untouched ng-pristine ng-valid">

                                    </fieldset>
                                </div>
                            </div>
                        </div>
                    </form>
                    <br><br>
                </div>
                <div class="overlay-footer">
                    <div class="right-align overlay-footer-content-rt">
                        <button class="btn btn-default" type="button" (click)="closeOverlay($event)">Close</button>
                        <button *ngIf="enableData" class="btn btn-primary btn-ladda btn-ladda-progres" id="saveConfig" [disabled]="btnDisabled" (click)="save('saveConfig')" type="button">Update</button>
                    </div>
                </div>
            </div>
            <!-- right panel script -->
        </div></div></div>
