<div id="payment" class="overlay-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <!-- <div class="hidden-xs simple-field-hf"></div> -->
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay($event)" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container" #test style="background-color: #fff;">
                <form class="form-vertical" [formGroup]="paymentForm">
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-lg"  *ngIf="feeDetails">
                                <thead>
                                <tr>
                                    <th class="col-sm-3">Fee Name</th>
                                    <th class="col-sm-3">Amount</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                    <td>{{feeDetails.feeAssignmentName}}</td>
                                    <td>{{feeDetails.netAmount}}</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                        {{myform | json}}
                        <legend class="text-semibold"><i class="icon-file-text2 position-left"></i> <span class="text-semibold text-uppercase"> Student Details</span></legend>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>First Name <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="firstName"  name="firstName" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.firstName"></div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Last Name <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="lastName"  name="lastName" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.lastName"></div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>DOB</label>
                                    <div class="form-group">
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="icon-calendar3"></i></span>
                                            <input type="date" #dob required  format="yyyy-MM-dd"  class="form-control pickadate-year-month-future" placeholder="DOB &hellip;">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Address Line 1 <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="addressLine1"  name="addressLine1" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.addressLine1"></div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Address Line 2</label>
                                    <input  type="text" formControlName="addressLine2"  name="addressLine2" required="required" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>City <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="city"  name="city" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.city"></div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>State <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="state"  name="state" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.state"></div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Pincode <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="pincode"  name="pincode" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.pincode"></div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Country <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="country"  name="country" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.country"></div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Email <span class="text-danger">*</span></label>
                                    <input  type="email" formControlName="emailAddress"  name="emailAddress" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.emailAddress"></div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Phone Number <span class="text-danger">*</span></label>
                                    <input  type="text" formControlName="primaryPhone"  name="primaryPhone" required="required" class="form-control">
                                    <div control-messages class="validation-error-label" [control]="paymentForm.controls.primaryPhone"></div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <div class="row invoice-payment" *ngIf="feeDetails">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="content-group">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="content-group">
                                            <h6 class="text-semibold">Total Amount: <span #amount> {{feeDetails.netAmount}} INR</span></h6>
                                            <h6 class="text-semibold">Payable Amount: <span #amount> {{feeDetails.netAmount - feeDetails.paidAmount}} INR</span></h6>
                                            <div class="table-responsive no-border">
                                                <table class="table">
                                                    <tbody>
                                                    <tr>
                                                        <th>Due Date:</th>
                                                        <td class="text-right">{{feeDetails.dueDate}}</td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="overlay-footer">
                        <div class="right-align overlay-footer-content-rt">
                            <button type="button" class="btn btn-default" (click)="closeOverlay($event)">Close</button>
                            <button type="button" [disabled]="!paymentForm.valid" class="btn btn-primary" (click)="pay()">Pay</button>
                        </div>
                    </div>
                </form>
                <!-- cd-panel-container -->
            </div>
        </div>
        <!-- right panel script -->
    </div>
</div>