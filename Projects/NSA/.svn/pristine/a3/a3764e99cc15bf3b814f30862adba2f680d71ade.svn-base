<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="nexrise.publication.in.nexrise.Gallery.VideosFragment">

    <!-- TODO: Update blank fragment layout -->
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <GridView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:stretchMode="columnWidth"
        android:numColumns="auto_fit"
        android:id="@+id/video_gridview"
        android:layout_margin="@dimen/margin5"
        android:horizontalSpacing="@dimen/margin5"
        android:verticalSpacing="@dimen/margin10"/>
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floating_action_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        android:background="@color/appColor"
        app:backgroundTint="@color/appColor"
        android:tint="@color/colorWhite"
        app:elevation="@dimen/elevation4"
        app:borderWidth="0dp"
        app:srcCompat="@android:drawable/ic_input_add" />
</FrameLayout>
</FrameLayout>
