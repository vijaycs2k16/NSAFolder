<div class="paddingMd">
    <table id="example" class="display" style="width:100%">
        <thead>
        <tr>
            <th >Name</th>
            <th >Status</th>
            <th >Score</th>
        </tr>
        </thead>
        <tbody>
        <%model.forEach(function(obj, index){%>
        <tr>
            <td class="text-center"><%=obj.student.studentName%></td>
            <td class="text-center">
                <% if(obj.isSubmit){ %>
                <span data-color="bgGreenLight" class="_tagLabelSmall">Attended</span>
                <% } else { %>
                <span data-color="bgRed" class="_tagLabelSmall">Not Attended</span>
                <% }%>

            </td>
            <td class="text-center"><%=obj.score%></td>
        </tr>
        <% }) %>
        </tbody>
        <tfoot>
        <tr>
            <th >Name</th>
            <th >Status</th>
            <th >Score</th>
        </tr>
        </tfoot>
    </table>
    <div class="formContentWrap">
        <div class="formContentTitle">
            <span class="closeBtn backBtn"></span>
        </div>
    </div>
</div>