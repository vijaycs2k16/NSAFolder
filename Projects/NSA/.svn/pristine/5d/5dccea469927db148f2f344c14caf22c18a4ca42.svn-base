<div class="form-holder _newForm">
    <form method="">
        <h4 class="_dialogMainTitle">Edit Batch </h4>
        <div class="_formWrap _enlargedItemMargin">
            <div class="_formBlock _equal">
                <div class="_animateInputBox">
                    <input type="text"
                           class="_animate "
                           id="name"
                           required
                           value="<%=model.batchName%>"/>
                    <label class="_animate">Batch Name<span class="text-danger">*</span></label>
                </div>
                <div class="_animateSelectBox _withInfo">
                    <span class="_selectTitle">Select Center<span class="text-danger">*</span></span>
                    <div class="_newSelectListWrap">
                        <a id="center"
                           name="center"
                           class="current-selected"
                           data-id="<%=model.center ? model.center._id: null%>"
                           href="javascript:;"><%=model.center ? model.center.centerName : 'Select Center'%></a>
                    </div>
                    <span class="tips icon-info" data-id="center"></span>
                </div>
                <div class="_animateSelectBox _withInfo">
                    <span class="_selectTitle">Select Course<span class="text-danger">*</span></span>
                    <div class="_newSelectListWrap">
                        <a id="course"
                           name="course"
                           class="current-selected"
                           data-id="<%=model.course ? model.course._id: null%>"
                           href="javascript:;"><%=model.course ? model.course.courseName : 'Select Course'%> </a>
                    </div>
                    <span class="tips icon-info" data-id="course"></span>
                </div>
                <div class="_animateTextareaBox ">
                    <div class="datafield _inputwrap">
                        <input class="_animate extrainfo"
                               type="text"
                               name="startDate"
                               id="startDate"
                               value="<%=model.startDate%>"
                               readonly />
                    </div>
                    <label for="startDate" class="_animate">Start Date<span class="text-danger">*</span></label>
                </div>
                <!--<div class="_animateTextareaBox ">
                    <div class="datafield _inputwrap">
                        <input class="_animate extrainfo"
                               type="text"
                               name="endDate"
                               id="endDate"
                               value="<%=model.endDate%>"
                               readonly />
                    </div>
                    <label for="endDate" class="_animate">End Date</label>
                </div>-->
                <div class="_animateInputBox">
                    <span class="_staticLabel">End Date</span>
                    <span id="endDate" class="_staticInfo"><%=model.endDate%></span>
                </div>
                <div class="_animateSelectBox _withInfo">
                    <span class="_selectTitle">Batch Status</span>
                </div>
                <div class="container">
                    <div class="canReadAndWrite row">
                        <div class="_circleRadioRadianceWrap col m-6">
                            <div class="_tipsWrap">
                                <label for="yes" class="">Active </label>
                                <input type="radio"
                                       name="question"
                                       id="yes"
                                       class="_circleRadioRadiance"
                                <%=model.batchStatus ? checked='checked' : ''%>
                                value="1">
                                <label for="yes"
                                       class="_circleRadioRadianceLabel">
                                </label>
                            </div>
                        </div>
                        <div class="_circleRadioRadianceWrap col m-6">
                            <div class="_tipsWrap">
                                <label for="no" class="">Deactive</label>
                                <input type="radio"
                                       name="question"
                                       id="no"
                                       value="0"
                                <%=!model.batchStatus ? checked='checked' : ''%>
                                class="_circleRadioRadiance">
                                <label for="no"
                                       class="_circleRadioRadianceLabel">
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>