<div class="panel panel-default">
    <div class="panel-heading">
        <h6 class="panel-title"><span class="text-semibold text-uppercase"><i class="glyphicon glyphicon-bell position-left"></i>OnBoard Notifications </span><i class="icon-arrow-right15 text-pink-600"></i> <small>List of Messages </small></h6>
        <div class="heading-elements" style="top:32px;">
            <input type="button" value="OnBoard Notification" *ngIf="enable"  data-size="75%" data-title="OnBoard Notification" data-toggle="from-right" data-id="#onboardModel" class="btn btn-sm btn-primary" (click)="openOverlay($event)"/>

        </div>
    </div>
    <input hidden id="warningButton" (click)="requested_warning()" type="button" style="display: none;">
    <input hidden id="editButton" (click)="getNotificationById($event)" data-size="75%" data-id="#onboardModel" type="button" style="display: none;">
    <div class="panel-body">
        <table class="table datatable-onboard">
            <thead>
            <tr>
                <th data-priority="1">Notification Name</th>
                <th>Total Count</th>
               <!-- <th>Downloads</th>-->
                <th>Current Logins</th>
                <th>Status</th>
                <th data-priority="2">Actions</th>
            </tr>
            </thead>
        </table>
    </div>
</div>

<!-- Primary modal -->

<div id="onboardModel" class="overlay-popup footer-popup" style="display: none;">
    <!-- RIGHT PANEL STARTS -->
    <div class="cd-panel">
        <div class="cd-panel-header">
            <!-- <div class="hidden-xs simple-field-hf"></div> -->
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay($event)" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container">
                <form action="#" class="form-horizontal from-validate" [formGroup]="onBoardNotificationform">
                    <div class="modal-body">
                            <div class="form-group" [ngClass]="{'not-active': viewOnboard}">
                                <label class="control-label col-md-3">Notification Title <span class="text-danger">*</span></label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" placeholder="Enter Title" formControlName="notificationName">
                                    <div control-messages class="validation-error-label" [control]="onBoardNotificationform.controls.notificationName"></div>
                                </div>
                            </div>

                        <div class="form-group" [ngClass]="{'not-active': viewOnboard}">
                            <label class="control-label col-md-3">Login Details For <span class="text-danger">*</span></label>
                            <div class="col-md-9">
                               <label class="radio-inline">
                                    <input type="radio" name="userTypes" id="student" (click)="checkLogins('false')"
                                           class="styled radio1" [value]="false" [checked]="!userTypes || userTypes === 'Student'">Individual Student Login
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="userTypes" id="Parent" (click)="checkLogins('true')"
                                           class="styled radio2" [value]="true" [checked]="!userTypes || userTypes === 'Parent'">Parent Login
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                                <label class="control-label col-md-3">Message For <span class="text-danger">*</span> </label>
                                <div class="col-md-9">
                                    <p-tree  [value]="data">
                                        <template let-node  pTemplate type="default">
                                            <input type="hidden" formControlName="classes">
                                            <input type="checkbox" [disabled]="viewOnboard" [(ngModel)]="node.selected" [ngModelOptions]="{standalone: true}" [indeterminate]="node.partialSelection" (ngModelChange)="nodeSelected(node)"><span style="margin-left: 6px">{{node.label}}</span>
                                        </template>
                                    </p-tree>
                                </div>
                            </div>
                            <div class="form-group" formGroupName="notify">
                                <label class="control-label col-md-3">Notifications
                                </label>
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <div class="col-sm-12">
                                            <div class="row">
                                                <div class="col-md-3 sms hide" [ngClass]="{'not-active': smsObject}">
                                                    <div class="form-group">
                                                        <label class="checkbox-inline" style="font-size:12px;">
                                                            <input  #sms type="checkbox" class="smsChecked">SMS
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-3 email hide">
                                                    <div class="form-group">
                                                        <label class="checkbox-inline" style="font-size:12px;">
                                                            <input #email type="checkbox" class="emailChecked">Email
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-3 push hide">
                                                    <div class="form-group">
                                                        <label class="checkbox-inline" style="font-size:12px;">
                                                            <input #push type="checkbox" class="pushChecked">App
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                    </div>
                </form>
                <div class="overlay-footer">
                    <div class="right-align overlay-footer-content-rt">
                        <button type="button" class="btn btn-sm btn-default" (click)="closeOverlay($event)">Close</button>
                        <button type="submit" *ngIf="!viewOnboard"  id="Sent" [disabled]="!onBoardNotificationform.valid" (click)="saveNotification('Sent')" class="btn btn-primary btn-ladda btn-ladda-progress Sent" data-style="expand-right" data-spinner-size="20"><span class="ladda-label">Send</span></button>
                        <!--<button type="submit" id="Draft" [disabled]="!onBoardNotificationform.valid" (click)="saveNotification('Draft')" class="btn btn-primary btn-ladda btn-ladda-progress Draft" data-style="expand-right" data-spinner-size="20"><span class="ladda-label">Save as Draft</span></button>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /primary modal -->
</div>


