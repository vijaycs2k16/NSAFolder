<style>
    .forms {
        margin: 0 auto;
        padding: 2em;
    }
    .wrap {
        width: 100%;
        overflow:auto;
    }

    .fleft {
        float:left;
        width: 50%;
    }

    .fright {
        float: right;
        width: 50%;
    }

    .vr {
        width:1px;
        background-color:#000;
        position:absolute;
        top:45px;
        bottom:0;
        left:50%;
        right:50%;
    }

    .bix-td-option {
        vertical-align: middle;
        padding: 7px;
    }

    .bix-td-qno {
        line-height: 1.7;
        width: 25px;
    }
    .bix-div-container {
        padding: 7px;
        line-height: 1.7;
    }
    .bix-div-container p {
        line-height: 1.7;
    }



    section {
        margin: 4px auto;
        padding: 2em;
        max-width: 870px;
        background: #fff;
        box-shadow:  unset !important;
        border-bottom: none !important;
    }

    img.Wirisformula { width:25px !important; height:19px !important;}

    section {
        max-width: inherit !important;
    }

</style>

