<% employeesCollection.forEach(function(employee, index){ %>
<tr data-index="<%=index + 1%>" data-id="<%=employee._id%>">
    <td class="notForm centerCell">
        <label class="_customCHeckbox">
            <input type="checkbox" value="<%=employee._id%>" class="checkbox"/>
            <span></span>
        </label>
    </td>
    <td style="text-align: center;"><%=employee.employeeId || '-'%></td>
    <td style="word-break: break-all;"><%=employee.name.first%> <%=employee.name.last%></td>
    <td class="notForm"><a href="skype:<%=employee.workPhones.mobile%>?call"><%=employee.workPhones.mobile%></a></td>
    <td><%=employee.workEmail%></td>
    <td><%=employee.password%></td>
    <td><%=(employee.userName) ? employee.userName : ''%></td>
    <td><%=(employee.manager && employee.manager.name) ? employee.manager.name.first + ' ' + employee.manager.name.last : ''%></td>
    <td><%=(employee.createdBy&&employee.createdBy.date)?( employee.createdBy.date + ((employee.createdBy.user) ?(' (' +
        employee.createdBy.user + ')') : '')):''%>
    </td>
    <td><%=(employee.editedBy&&employee.editedBy.date)?( employee.editedBy.date + ((employee.editedBy.user) ?(' (' +
        employee.editedBy.user + ')') : '')):''%>
    </td>

</tr>
<% }) %>

