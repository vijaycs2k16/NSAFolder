<% collection.forEach(function(item, index){ %>
<tr data-id="<%=item._id%>">
    <td class="notForm">
        <label class="_customCHeckbox">
            <input type="checkbox" value="<%=item._id%>" class="checkbox"/>
            <span></span>
        </label>
    </td>

    <td data-content="name" class="editable"><%=item.transactionDate ? item.transactionDate : ''%></td>
    <td class="editable"><%=item.reason%></td>
    <td class="editable"><%=item.product ? item.product.name : ''%></td>
    <td class="editable"><%=item.bundles ? item.bundles[0].quantity : ''%></td>
</tr>
<% }) %>

