<div id="mark-upload-config" class="overlay-popup footer-popup" style="display: none;">
    <div class="cd-panel">
        <div class="cd-panel-header">
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay()" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container">
                <div class="panel panel-default" [hidden]="edit">
                    <div class="filter-panel-body">
                        <form class="form-horizontal">
                            <div class="form-group col-md-3">
                                <label class="control-label col-md-3">Class <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #classSelect id="class-name" class="select-change" (click)="getSectionByClass()" data-width="100%">
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4" [hidden]="!classId">
                                <label class="control-label col-md-3">Section <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #sectionSelect id="section-name" data-width="100%" class="select-change" (click)="getExamsByClassAndSections()">
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4" [hidden]="!classId || !sectionId">
                                <label class="control-label col-md-3">Exam <span class="text-danger">*</span>
                                </label>
                                <div class="col-md-8">
                                    <div class="row">
                                        <select #examSelect id="exam-name" class="select-change" (click)="getUsersByClassAndSection()" data-width="100%">
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="panel panel-default" *ngIf="publish">
                    <div class="filter-panel-body">
                        <form class="form-horizontal">
                            <div class="form-group col-md-6">
                                <label class="control-label col-md-2">Channels
                                </label>
                                <label class="checkbox-inline sms hide" style="font-size:12px;">
                                    <input  #sms type="checkbox" class="smsChecked"  checked="checked">SMS
                                </label>
                                <label class="checkbox-inline email hide" style="font-size:12px;">
                                    <input #email type="checkbox" class="emailChecked">Email
                                </label>
                                <label class="checkbox-inline push hide" style="font-size:12px;">
                                    <input #push type="checkbox" class="pushChecked">App
                                </label>
                            </div>
                        </form>
                    </div>
                </div>
                <input type="hidden" id="upload-mark" (click)="uploadMark($event)">
                <div class="modal-body" [hidden]="!classId || !sectionId || !examId">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h5 class="panel-title">Upload Marks</h5>
                        </div>

                        <div class="panel-body">
                            <table id="example" class="table display mark-upload" width="100%"></table>
                        </div>
                    </div>
                </div>
            </div>
            <hr/>
            <div class="overlay-footer">
                <div class="right-align overlay-footer-content-rt">
                    <button type="button" (click)="closeOverlay()" class="btn btn-default">Close</button>
                    <button *ngIf="publish" type="submit" id="publishButton" (click)="publisMarkList($event)" class="btn btn-primary btn-ladda btn-ladda-progress" data-style="expand-right" data-spinner-size="20"><span class="ladda-label">Publish</span></button>
                </div>
            </div>
        </div>
    </div>
</div>