<!-- Generate mark sheet starts here -->
<div id="generate-mark-sheet" class="overlay-popup footer-popup" style="display: none;">
    <div class="cd-panel">
        <div class="cd-panel-header">
            <div class="simple-field-hf">
            </div>
            <a (click)="closeOverlay()" class="cd-panel-close">Close</a>
        </div>
        <div class="cd-panel-container">
            <div class="cd-panel-content" id="side-panel-container">
                <form class="form-horizontal" action="#">
                    <!--  <div class="panel-body"> -->
                    <div class="form-group">
                        <div class="col-md-12">
                            <div class="panel panel-flat table-responsive">
                                <table id="curriculum-students-dataTable"
                                       class="display datatable-export datatable-mark"
                                       cellspacing="0" width="100%">
                                    <thead>
                                    <tr>
                                        <th colspan="2">Term 1</th>
                                        <th colspan="4">Class 1 - Section A</th>
                                        <th colspan="3">Periodic Assessment</th>
                                        <th colspan="3">Date : 26 Aug 17</th>
                                    </tr>
                                    <tr>
                                        <th rowspan="2">Name</th>
                                        <th rowspan="2">Subjects</th>
                                        <th colspan="2">Periodic Test</th>
                                        <th colspan="1">C.Score</th>
                                        <th colspan="3">SEA</th>
                                        <th colspan="3">NoteBook.</th>
                                        <th colspan="1">Grade</th>
                                    </tr>
                                    <tr>

                                        <th>PT 1</th>
                                        <th>PT 2</th>
                                        <th>PT (10)</th>
                                        <th>Lab</th>
                                        <th>Concept</th>
                                        <th>Total (5)</th>
                                        <th>W</th>
                                        <th>A</th>
                                        <th>Total (5)</th>
                                        <th>G</th>
                                    </tr>
                                    </thead>

                                </table>


                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <hr/>
        <div class="overlay-footer">
            <div class="right-align overlay-footer-content-rt">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <input type="button" value="Proceed" data-size="100%" data-title="Grade Assessment"
                       data-toggle="from-right" data-id="#add-mark-upload" class="btn btn-sm btn-primary heading-btn"
                       (click)="genProgressCard($event)"/>
            </div>
        </div>
    </div>
    <!-- </div> -->
</div>
<!-- generate mark sheet ends here -->