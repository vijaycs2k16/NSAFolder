<table class="_productTable left-alignment">
    <thead>
    <tr class="productListItemsColumns _deleteHeader">
        <th class="" data-sort="Batch">
            Batch Name
        </th>
        <th>Subject Name</th>
        <th data-sort="unitPrice">
            Topic Name
        </th>
        <th>Faculty Name</th>
        <th data-sort="taxes">
            Class Start Time
        </th>
        <th data-sort="taxes">
            Class End Time
        </th>
        <th data-sort="remove">
            Remove
        </th>
    </tr>
    </thead>
    <tbody id="productList">
        <% if(data) { %>
        <% _.forEach(data, function(value, index) { %>
        <tr class="productItem deleteRow left-alignment _productRow" id="<%=value._id%>">
            <td data-name="productName" class="text-left">
                <div class="_animateSelectBox">
                    <div class="_newSelectListWrap">
                        <a id="batch"
                           class="current-selected batch"
                           value="<%=value.course ? value.course._id: null%>"
                           data-id="<%=value.batch ? (value.batch._id + '-' + value.course._id): null%>"
                           href="javascript:;"><%=value.batch ? value.batch.batchName + ' - ' + value.course.courseName : 'Select'%></a>
                        </a>
                    </div>
                    <span class="tips " data-id="batch"></span>
                </div>
            </td>
            <td data-name="productName" class="text-left">
                <div class="_animateSelectBox">
                    <div class="_newSelectListWrap">
                        <a id="subject"
                           name="subject"
                           class="current-selected"
                           data-id="<%=value.subject ? value.subject._id: null%>"
                           href="javascript:;"><%=value.subject ? value.subject.subjectName : 'Subject Name'%></a>
                    </div>
                    <span class="tips " data-id="subject"></span>
                </div>
            </td>
            <td data-name="productName" class="text-left">
                <div class="_animateSelectBox">
                    <div class="_newSelectListWrap">
                        <a id="topics"
                           name="topics"
                           class="current-selected"
                           data-id="<%=value.topicData ? value.topicData._id : null %>"
                           href="javascript:;"><%=value.topicData? value.topicData.name : 'Topic Name'%></a>
                    </div>
                    <span class="tips" data-id="topics"></span>
                </div>
            </td>
            <td data-name="productName" class="text-left">
                <div class="_animateSelectBox">
                    <div class="_newSelectListWrap">
                        <a id="faculty"
                           name="faculty"
                           class="current-selected"
                           data-id="<%=value.faculty ? value.faculty._id: null%>"
                           href="javascript:;"><%=value.faculty ? value.faculty.name.first + ' ' +  value.faculty.name.first : 'Faculty Name'%></a>
                    </div>
                    <span class="tips " data-id="subject"></span>
                </div>
            </td>
            <td data-name="productName" class="">
                <div class="_animateinputbox _withtime _withinfo">
                    <div class="inputdateactionblock" style="
                               position: relative;
                               display: block;
                               padding: 0 20px 0 0;
                               border: none;
                               min-height: 26px;
                               cursor: pointer;
                               border-bottom: 1px solid #c8c8d3;
                               font-size: 13px;
                               color: #999 !important;
                               ">
                        <input maxlength="2"
                               name="time"
                               class="timepickerOne1"
                               id="timepickerOne1<%=index%>"
                               value="<%=value.classStartTime ? value.classStartTime: null%>"
                               readonly/>
                    </div>
                    <span class="tips " data-id="classStartTime"></span>
                </div>
            </td>
            <td data-name="productName" class="">
                <div class="_animateinputbox _withtime _withinfo">
                    <div class="inputdateactionblock" style="
                               position: relative;
                               display: block;
                               padding: 0 20px 0 0;
                               border: none;
                               min-height: 26px;
                               cursor: pointer;
                               border-bottom: 1px solid #c8c8d3;
                               font-size: 13px;
                               color: #999 !important;
                               ">
                        <input maxlength="2"
                               name="time"
                               class="timepickerTwo2"
                               id="timepickerTwo2<%=index%>"
                               value="<%=value.classEndTime ? value.classEndTime: null%>"
                               readonly/>
                    </div>
                    <span class="tips " data-id="classEndTime"></span>
                </div>
            </td>

            <td class="deleteCell" style="text-align: start">
                <span title="Delete" class="icon-close5 removeJob"></span>
            </td>
        </tr>
        <% }) }%>

    </tbody>
    <tr class="" id="newItem">
        <td class="addProductItem">
            <a href="javascript:;" class="btn blue slim" id="addBatch">Add Batches</a>
        </td>
    </tr>
</table>
<div id="totalAmountContainer" class="_totalAmountContainer _createPopup"></div>
